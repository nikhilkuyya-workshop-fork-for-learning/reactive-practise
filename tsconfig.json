{
  "compilerOptions": {
    "lib": ["DOM","ESNext","ScriptHost"],
    "target": "ESNext",
    "module": "ESNext",
    "preserveWatchOutput": true,
    "pretty": true,
    "traceResolution": false,
    "moduleDetection": "auto",
    "extendedDiagnostics": false,
    "listEmittedFiles": true,
    "listFiles": false,
    "outDir": "dist"
  },
  "include": ["src/**.ts"],
  "exclude": ["dist/**"]
}
